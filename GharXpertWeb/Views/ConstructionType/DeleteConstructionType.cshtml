@model GharXpertWeb.Models.Dto.ConstructionTypeDTO
<form asp-action="DeleteConstructionType">
    <br />
    <div class="container border p-3">
        <h1 class="text-primary">Delete ConstructionType</h1>
        <hr />

        <input hidden asp-for="Cno" />

        <div class="row">
            
            <div class="col-2">
                <label asp-for="Cno" class="control-label pt-2" style="font-size:20px;"></label>
            </div>

            <div class="col-10 pb-3">
                <input asp-for="Cno" disabled class="form-control" />
            </div>

            <div class="col-2">
                <label asp-for="Ctype" class="control-label pt-2" style="font-size:20px;"></label>
            </div>

            <div class="col-10 pb-3">
                <input asp-for="Ctype" disabled class="form-control" />
            </div>

            <div class="col-5 offset-2">
                <a asp-action="IndexConstructionType" asp-controller="ConstructionType" class="btn-primary btn form-control">Back To List</a>
            </div>

            <div class="col-5">
                <input type="submit" value="Delete" class="btn btn-danger form-control" />
            </div>
        </div>
    </div>
</form>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}